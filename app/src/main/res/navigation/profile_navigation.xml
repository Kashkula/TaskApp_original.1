<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profile_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.taskapp_orig.ui.profileFragment.ProfileFragmentPhoto"
        android:label="fragment_profile_photo"
        tools:layout="@layout/fragment_profile_photo" />
    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.taskapp_orig.ui.profileFragment.ProfileFragmentVideo"
        android:label="fragment_profile_video"
        tools:layout="@layout/fragment_profile_video" />
    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.taskapp_orig.ui.profileFragment.ProfileFragmentSettings"
        android:label="fragment_profile_settings"
        tools:layout="@layout/fragment_profile_settings" />
